{
  "name": "twosteps",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run twosteps:serve-ssr",
    "serve:ssr": "node dist/twosteps/server/main.js",
    "build:ssr": "ng build && ng run twosteps:server && gzipper compress --incremental --exclude jpeg,png,ico ./dist",
    "prerender": "ng run twosteps:prerender",
    "start:heroku": "node dist/twosteps/server/main",
    "postinstall": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.3",
    "@angular/common": "~12.2.3",
    "@angular/compiler": "~12.2.3",
    "@angular/core": "~12.2.3",
    "@angular/forms": "~12.2.3",
    "@angular/platform-browser": "~12.2.3",
    "@angular/platform-browser-dynamic": "~12.2.3",
    "@angular/platform-server": "~12.2.3",
    "@angular/router": "~12.2.3",
    "@angular/service-worker": "~12.2.3",
    "@nguniversal/express-engine": "^12.1.0",
    "compression": "^1.7.4",
    "express": "^4.15.2",
    "leaflet": "^1.7.1",
    "ngx-twitter-timeline": "^0.1.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.3",
