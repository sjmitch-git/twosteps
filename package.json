    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/leaflet": "^1.7.5",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "gzipper": "^5.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
